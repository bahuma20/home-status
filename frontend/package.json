{
  "name": "table-widget",
  "version": "1.0.0",
  "description": "",
  "main": "jquery.min.js",
  "dependencies": {
    "moment": "^2.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.10.1",
    "@babel/preset-env": "^7.9.5",
    "autoprefixer": "^9.7.6",
    "babel-loader": "^8.1.0",
    "cpx": "^1.5.0",
    "node-sass": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "onchange": "^6.1.1",
    "postcss": "^7.0.27",
    "postcss-cli": "^7.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "scripts": {
    "css:autoprefixer": "postcss -u autoprefixer -r ../public/app/*.css",
    "css:scss": "node-sass --output-style compressed -o ../public/app scss",
    "build:css": "npm run css:scss && npm run css:autoprefixer",
    "build:js": "webpack --mode=production",
    "build:copy": "cpx index.html ../public/app",
    "build": "run-s build:*",
    "watch:css": "onchange \"scss\" -- npm run build:css",
    "watch:js": "onchange \"js\" -- webpack --mode=development",
    "watch:html": "onchange \"index.html\" -- npm run build:copy",
    "watch": "run-p watch:*"
  },
  "browserslist": [
    "defaults"
  ],
  "author": "",
  "license": "ISC"
}
